{
  "chains": [
    {
      "name": "Ethereum",
      "type": "ethereum",
      "id": "0",
      "endpoint": "${ETH_URL}",
      "from": "${GANACHE_ADDR}",
      "opts": {
        "bridge": "${ETH_BRIDGE_ADDR}",
        "erc20Handler": "${ETH_ERC20_HANDLER_ADDR}",
        "genericHandler": "${ETH_GEN_HANDLER_ADDR}",
        "maxGasPrice": "20000000",
        "gasLimit": "8000000",
        "gasMultiplier": "1",
        "http": "true",
        "startBlock": "0",
        "blockConfirmations": "2"
      },
      "info": {
        "erc20Address": "${ETH_ERC20_ADDR}"
      }
    },
    {
      "name": "Avalanche",
      "type": "ethereum",
      "id": "1",
      "endpoint": "${AVAX_URL}",
      "from": "${GANACHE_ADDR}",
      "opts": {
        "bridge": "${AVAX_BRIDGE_ADDR}",
        "erc20Handler": "${AVAX_ERC20_HANDLER_ADDR}",
        "genericHandler": "${AVAX_GEN_HANDLER_ADDR}",
        "maxGasPrice": "20000000",
        "gasLimit": "8000000",
        "gasMultiplier": "1",
        "http": "true",
        "startBlock": "0",
        "blockConfirmations": "2"
      },
      "info": {
        "erc20Address": "${AVAX_ERC20_ADDR}"
      }
    }
  ]
}
